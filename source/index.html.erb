---
title: middleman-personal
---

<h2>Work</h2>
<ul class="unstyled pl0">
  <% data.work.each do |item| %>
    <li>
      <h3>
        <%= link_to(item.name, item.url, target: "_blank") %>
        <small><%= item.date %></small>
      </h3>
      <p><%= item.description %></p>
      <% if item.source %>
        <%= link_to("View Source", item.source, target: "_blank") %>
      <% end %>
    </li>
  <% end %>
</ul>

<h2>Labs</h2>
<ul class="unstyled pl0">
  <% data.labs.each do |item| %>
    <li>
      <h3>
          <%= link_to(item.name, item.url, target: "_blank") %>
      </h3>
      <p><%= item.description %></p>
      <% if item.source %>
        <%= link_to("View Source", item.source, target: "_blank") %>
      <% end %>
    </li>
  <% end %>
</ul>